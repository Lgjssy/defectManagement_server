<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.mapper.TPhistory">
    <select id="findTPByTestPlanNum" resultType="com.example.demo.entity.TPHistory">
        select *
        from tes_plan_history
        where Text_Plan_Id=#{TestPlanNum}
    </select>
</mapper>